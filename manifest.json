{
    "name": "Comments_Removed",
    "version": "0.1",
    "description": "Remove comments on YouTube",
    "content_scripts": [{
        "matches": [
            "https://www.youtube.com/watch?v=*"
        ],
        "js": ["comments_remove.js"]
    }],
    "browser_action": {
        "default_title": "Remove comments on YouTube"
    },
    "manifest_version": 2
}